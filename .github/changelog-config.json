{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement", "feat"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test", "tests"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["maintenance", "chore", "refactor"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    }
  ],
  "ignore_labels": [
    "ignore",
    "wontfix",
    "duplicate",
    "invalid"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "^(feat|feature)(\\(.+\\))?: (.+)",
      "target": "$1",
      "flags": "gu"
    },
    {
      "pattern": "^(fix|bugfix)(\\(.+\\))?: (.+)",
      "target": "$1",
      "flags": "gu"
    }
  ],
  "transformers": []
}
